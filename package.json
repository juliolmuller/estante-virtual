{
  "name": "estante-virtual",
  "version": "1.1.4",
  "private": true,
  "license": "MIT",
  "author": "Julio L. Muller",
  "homepage": "https://estantevirtual.vercel.app/",
  "type": "module",
  "scripts": {
    "postinstall": "node -e \"require('fs').existsSync('.env') || require('fs').copyFileSync('.env.example', '.env')\"",
    "prepare": "husky",
    "commit": "git-cz",
    "dev": "vite --open",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "bun run lint:node && bun run lint:app",
    "lint:app": "eslint -c src/eslint.config.js --fix src/",
    "lint:node": "eslint --fix server.js eslint.config.js vite.config.js",
    "start": "node server.js"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "bootstrap": "~5.1.3",
    "json-server": "~0.17.4",
    "pinia": "^2.3.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/tsconfig": "^0.7.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.23.0",
    "eslint-config-jlm": "^0.6.9",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "rollup-plugin-analyzer": "^4.0.0",
    "sass": "^1.86.0",
    "typescript": "^5.8.2",
    "vite": "^6.2.3",
    "vue-tsc": "^2.2.8"
  }
}
