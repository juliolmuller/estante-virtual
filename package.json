{
  "name": "estante-virtual",
  "version": "1.1.5",
  "private": true,
  "license": "MIT",
  "author": "Julio L. Muller",
  "homepage": "https://estantevirtual.vercel.app/",
  "type": "module",
  "scripts": {
    "postinstall": "node -e \"require('fs').existsSync('.env') || require('fs').copyFileSync('.env.example', '.env')\"",
    "prepare": "husky",
    "commit": "git-cz",
    "release": "semantic-release",
    "dev": "vite --open",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint": "bun run lint:node && bun run lint:app",
    "lint:app": "eslint -c src/eslint.config.js --fix src/",
    "lint:node": "eslint --fix server.js eslint.config.js vite.config.ts",
    "start": "node server.js"
  },
  "dependencies": {
    "axios": "^1.9.0",
    "bootstrap": "~5.1.3",
    "json-server": "~0.17.4",
    "pinia": "^3.0.2",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.2",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/tsconfig": "^0.7.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.25.1",
    "eslint-config-any": "^1.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "rollup-plugin-analyzer": "^4.0.0",
    "sass": "^1.87.0",
    "semantic-release": "^24.2.3",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vue-tsc": "^2.2.10"
  }
}
